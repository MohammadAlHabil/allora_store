{
  "servers": {
    "shadcn-remote": {
      "url": "https://www.shadcn.io/api/mcp",
      "type": "http"
    },
    "shadcn-local": {
      "command": "npx",
      "args": ["shadcn@latest", "mcp"],
      "type": "stdio"
    },
    "next-devtools": {
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"],
      "type": "stdio"
    },
    "Prisma-Local": {
      "command": "npx",
      "args": ["-y", "prisma", "mcp"],
      "type": "stdio"
    },
    // "Prisma-Remote": {
    //   "command": "npx",
    //   "args": ["-y", "mcp-remote", "https://mcp.prisma.io/mcp"],
    //   "type": "stdio"
    // },
    "Prisma-Remote": {
      "url": "https://mcp.prisma.io/mcp",
      "type": "http"
    },
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/"
    }
  }
}
